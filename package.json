{
  "name": "weather-app",
  "productName": "Weather App",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AirSodaz/weather-app.git"
  },
  "description": "A beautiful cross-platform weather application.",
  "author": {
    "name": "AirSodaz",
    "email": "asodaz@outlook.com"
  },
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"cross-env BROWSER=none npm run react:dev\" \"npm run electron:dev\"",
    "react:dev": "vite",
    "electron:dev": "wait-on tcp:5173 && tsc -p electron/tsconfig.json && electron .",
    "build": "tsc -b && vite build",
    "android:dev": "npm run tauri android dev",
    "android:build": "npm run tauri android build",
    "preview": "vite preview",
    "tauri": "tauri"
  },
  "build": {
    "directories": {
      "buildResources": "assets"
    }
  },
  "overrides": {
    "source-map": "0.7.4",
    "magic-string": "^0.30.17"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-shell": "^2.3.4",
    "@tauri-apps/plugin-window": "^2.0.0-alpha.1",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "electron-store": "^11.0.2",
    "framer-motion": "^12.24.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.0.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.6",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^39.2.7",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.0",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "wait-on": "^7.2.0"
  }
}